{
    "AWS AMI": {
        "prefix": "tf-aws_ami",
        "description": "Terraform aws_ami snippet",
        "body": [
            "data \"aws_ami\" \"nat_ami\" {",
            "most_recent = true",
            "executable_users = [\"self\"]",
            "filter {",
            "name = \"owner-alias\"",
            "values = [\"amazon\"]",
            "}",
            "filter {",
            "name = \"name\"",
            "values = [\"amzn-ami-vpc-nat*\"]",
            "}",
            "owners = [\"self\"]",
            "}"
        ]
    }
}