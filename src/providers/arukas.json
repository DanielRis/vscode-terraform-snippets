{
    "arukas": {
        "prefix":"tf-arukas",
        "description": "Defines Arukas provider",
        "body": [
            "provider \"arukas\" {",
            "   token   = \"${1:token}\"",
            "   secret  = \"${2:secret}\"",
            "}"
        ]
    },
    "arukas_container": {
        "prefix":"tf-arukas_container",
        "description": "Defines Arukas container",
        "body": [
            "resource \"arukas_container\" \"${1:arukas_container_id}\" {",
            "   name   = \"${2:myArukasName}\"",
            "}"
        ]
    }
}